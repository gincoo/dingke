.view_root {
  width: 100%;
  height: 100vh;
}

.view_title {
  width: 100%;
  display: flex;
  justify-content: center;
  align-content: center;
}

.view_btn {
  width: 100%;
  display: flex;
  justify-content: center;
  align-content: center;
}

.view_item_root {
  margin-top: 15px;
  margin-bottom: 10px;
  width: 100%;
  height: 30px;
  display: flex;
  flex-flow: row wrap;
  justify-content: center;
}

.view_item {
  margin-top: 15px;
  margin-bottom: 10px;
  width: 100%;
  height: 30px;
  display: flex;
  flex-flow: row wrap;
  justify-content: center;
}

.text_item {
  font-size: 14px;
}

.text_item_right {
  font-size: 12px;
  margin-left: 40px;
  padding-left: 10px;
  padding-right: 10px;
  align-items: center;
  color: rgb(18, 105, 235);
}
