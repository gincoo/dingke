
.root {
  width: 100%;
  height: 100vh;
  background-color: rgba(170, 231, 255, 0.062);
  margin-left: 10px;
}

.view_title {
  margin-left: 10px;
}

.text_title {
  font-size: 16px;
}

.view_item {
  margin-left: 30px;
  margin-top: 15px;
  margin-bottom: 10px;
  width: 100%;
  height: 30px;
  display: flex;
  flex-flow: row wrap;
  justify-content: flex-start center;
}

.text_item {
  font-size: 14px;
}
.text_item2 {
  font-size: 12px;
  margin-left: 40px;
  align-items: center;
  color: rgb(18, 105, 235);
}

.view_btn{
  width: 100%;
  flex-flow: row wrap;
  justify-content: center;
  display: flex;
}
